{
  "name": "rak",
  "version": "0.4.1",
  "description": "The source code for randomactsofkinase.com",
  "main": "/",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build-semantic-ui": "cd ./semantic && gulp build",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack/webpack.config.js",
    "build-windows": "cross-env NODE_ENV=production node_modules\\.bin\\webpack --config webpack/webpack.config.js",
    "lint": "tslint --project .",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "watch-static-into-build": "cpx \"../resources/**/*\" ../../../build/resources/main/ --watch",
    "test": "cross-env NODE_ENV=test karma start karma.unit.js",
    "coverage": "cross-env NODE_ENV=test karma start karma.coverage.js",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt"
  },
  "author": "Robert Futrell <robert.e.futrell@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "0.17.1",
    "datatables.net": "1.10.16",
    "datatables.net-dt": "1.10.16",
    "debounce": "1.1.0",
    "font-awesome": "4.7.0",
    "jquery-lazy": "1.7.7",
    "semantic-ui": "2.2.13",
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vue-toasted": "1.1.24",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.3",
    "copy-webpack-plugin": "4.3.1",
    "cpx": "1.5.0",
    "cross-env": "5.1.3",
