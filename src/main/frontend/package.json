    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine": "2.8.0",
    "karma": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "2.0.9",
